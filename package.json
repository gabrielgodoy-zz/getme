{
  "name": "getme",
  "version": "1.4.2",
  "description": "CLI utility for everyday tasks",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielgodoy/getme"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "./node_modules/.bin/babel ./src -d dist",
    "watch": "npm-watch",
    "test": "mocha --compilers js:babel-register --colors --reporter spec ./src/**/*.test.js",
    "test:watch": "npm test -- --watch",
    "coverage": "nyc --report lcov --report text _mocha --compilers js:babel-register ./src/**/*.js",
    "coveralls": "nyc report --reporter text-lcov | coveralls",
    "lint": "eslint './src/**/*.js'",
    "precommit": "npm run lint && npm test",
    "prepush": "npm run lint"
  },
  "nyc": {
    "exclude": [
      "src/**/*.test.js",
      "stubs/**/*.js"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "plugins": [
      "import"
    ],
    "rules": {
      "no-console": 0
    },
    "env": {
      "browser": true,
      "mocha": true
    }
  },
  "author": "Gabriel Godoy",
  "keywords": [
    "google",
    "maps",
    "places",
    "weather"
  ],
  "bin": {
    "getme": "./dist/index.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "preferGlobal": true,
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "axios": "^0.15.3",
    "babel-runtime": "^6.20.0",
    "chalk": "^1.1.3",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^2.9.0",
    "inquirer": "^3.0.6",
    "node-emoji": "^1.4.3",
    "oauth": "^0.9.15",
    "ora": "^1.2.0",
    "request": "^2.81.0",
    "speedtest-net": "^1.2.11",
    "thunkify": "^2.1.2",
    "urlencode": "^1.1.0"
  },
  "bugs": {
    "url": "https://github.com/gabrielgodoy/getme/issues",
    "email": "hi@gabrielgodoy.com"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.13.3",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "moxios": "^0.3.0",
    "nock": "^9.0.2",
    "npm-watch": "^0.1.7",
    "nyc": "^11.0.2",
    "rewire": "^2.5.2",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0"
  }
}
