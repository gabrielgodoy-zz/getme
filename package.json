{
  "name": "getme",
  "version": "1.2.13",
  "description": "CLI utility for everyday tasks",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielgodoy/getme"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "./node_modules/.bin/babel ./src -d dist",
    "watch": "npm-watch",
    "test": "mocha --colors --reporter spec ./src/**/*.test.js",
    "test:watch": "npm test -- --watch",
    "coverage": "istanbul cover --report text --config istanbul.yml _mocha ./src/**/*.js",
    "coveralls": "NODE_ENV=test istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly --config istanbul.yml -- -R spec ./src/**/*.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "lint": "eslint './src/**/*.js'",
    "precommit": "npm run lint && npm test"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "plugins": [
      "import"
    ],
    "rules": {
      "no-console": 0
    },
    "env": {
      "browser": true,
      "mocha": true
    }
  },
  "author": "Gabriel Godoy",
  "keywords": [
    "google",
    "maps",
    "places",
    "weather"
  ],
  "bin": {
    "getme": "./dist/index.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "preferGlobal": true,
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "node-emoji": "^1.4.3",
    "ora": "^0.4.0",
    "request": "^2.79.0",
    "speedtest-net": "^1.2.11"
  },
  "bugs": {
    "url": "https://github.com/gabrielgodoy/getme/issues",
    "email": "hi@gabrielgodoy.com"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.12.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "nock": "^9.0.2",
    "npm-watch": "^0.1.7",
    "rewire": "^2.5.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0"
  }
}
